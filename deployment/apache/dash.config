<VirtualHost *:80>

    ServerName dash.example.com
    ErrorLog ${APACHE_LOG_DIR}/error.log
    CustomLog ${APACHE_LOG_DIR}/access.log combined

    WSGIDaemonProcess dashapp threads=5 user=www-data group=www-data home=/var/www/climate_narrative
    WSGIScriptAlias / /var/www/climate_narrative/deployment/apache/dash.wsgi
   
    <Directory /var/www/climate_narrative>
        WSGIProcessGroup dashapp
        WSGIApplicationGroup %{GLOBAL}
        Require all granted
    </Directory>

</VirtualHost>

